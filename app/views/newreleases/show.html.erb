<div>
  <p id="notice"><%= notice %></p>
</div>
<div>
  <h1>New Release Details</h1>
</div>
<div>
  <table>
    <tr>
      <td>Item Code</td>
      <td><%= @newrelease.item_code %></td>
    </tr>
    <tr>
      <td>Discount Code</td>
      <td><%= @newrelease.discount_code %></td>
    </tr>
    <tr>
      <td>Title</td>
      <td><%= @newrelease.title %></td>
    </tr>
    <tr>
      <td>Price</td>
      <td><%= @newrelease.price %></td>
    </tr>
    <tr>
      <td>Vendor</td>
