<div>
  <p id="notice"><%= notice %></p>
</div>
<div class="row">
  <div>
    <h2>Current Subscribers</h2>
    <h6>Total Subscribers: <%= @subscribers.count %></h6>
    <h6><%= button_to "Add Subscriber", new_subscriber_path, class: "button success" %></h6>
      <table> 
      <tr>
        <th>Name Index</th>
        <th>Name</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        
      </tr>
      <% @subscribers.each do |subscriber| %>
      
      <tr>
        <td><%= link_to subscriber.index, subscriber_path(subscriber) %></td>
        <td><%= link_to subscriber.full_name, subscriber_path(subscriber) %></td>
        <td><%= link_to subscriber.address, subscriber_path(subscriber) %></td>
        <td><%= link_to subscriber.city, subscriber_path(subscriber) %></td>
        <td><%= link_to subscriber.state, subscriber_path(subscriber) %></td>
        <td><%= link_to subscriber.f_zip, subscriber_path(subscriber) %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>


